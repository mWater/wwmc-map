    </tr>
    <tr class="danger">
        <th scope="row">Pounch snails</th>
        {{#each data}}
        <td>
            {{#if pounchsnails}}
            <span class="glyphicon glyphicon-ok" aria-hidden="true"></span>
            {{/if}}
        </td>
        {{/each}}
    </tr>
    <tr class="danger">
        <th scope="row">Tubiflex worms</th>
        {{#each data}}
        <td>
            {{#if tubiflexworms}}
            <span class="glyphicon glyphicon-ok" aria-hidden="true"></span>
            {{/if}}
        </td>
        {{/each}}
    </tr>
    </tbody>
</table>
{{/if}}
</div>
<div style="height:280px;overflow: auto;">
<br>
{{#if hasNoData}}
No data
{{else}}
<table id="speciesTable" class="table table-bordered table-condensed">
    <thead>
    <tr>
        <th>Species</th>
        {{#each data}}
            <th>{{date}}</th>
        {{/each}}
    </tr>
    </thead>
    <tbody>
    <tr class="success">
        <th scope="row">Caddisflies</th>
        {{#each data}}
            <td>
            {{#if caddisflies}}
            <span class="glyphicon glyphicon-ok" aria-hidden="true"></span>
            {{/if}}
            </td>
        {{/each}}
    </tr>
    <tr class="success">
        <th scope="row">Dobsonflies</th>
        {{#each data}}
        <td>
            {{#if dobsonflies}}
            <span class="glyphicon glyphicon-ok" aria-hidden="true"></span>
            {{/if}}
        </td>
        {{/each}}
    </tr>
    <tr class="success">
        <th scope="row">Mayflies</th>
        {{#each data}}
        <td>
            {{#if mayflies}}
            <span class="glyphicon glyphicon-ok" aria-hidden="true"></span>
            {{/if}}
        </td>
        {{/each}}
    </tr>
    <tr class="success">
        <th scope="row">Stoneflies</th>
        {{#each data}}
        <td>
            {{#if stoneflies}}
            <span class="glyphicon glyphicon-ok" aria-hidden="true"></span>
            {{/if}}
        </td>
        {{/each}}
    </tr>
    <tr class="warning">
        <th scope="row">Craneflies</th>
        {{#each data}}
        <td>
            {{#if craneflies}}
            <span class="glyphicon glyphicon-ok" aria-hidden="true"></span>
            {{/if}}
        </td>
        {{/each}}
    </tr>
    <tr class="warning">
        <th scope="row">Dragonflies</th>
        {{#each data}}
        <td>
            {{#if dragonflies}}
            <span class="glyphicon glyphicon-ok" aria-hidden="true"></span>
            {{/if}}
        </td>
        {{/each}}
    </tr>
    <tr class="warning">
        <th scope="row">Scuds</th>
        {{#each data}}
        <td>
            {{#if scuds}}
            <span class="glyphicon glyphicon-ok" aria-hidden="true"></span>

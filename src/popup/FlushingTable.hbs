{{#if hasNoData}}
        No data
    {{else}}
    <table id="ploggingHistoryTable" class="table table-bordered">
        <thead>
        <tr>
            <th width="250">Date</th>
            <th>Affiliation</th>
            <th>Affiliation Name</th>
            <th>Participants</th>
            <th>Poster?</th>
        </tr>
        </thead>
        <tbody>
        {{#each data}}
            <tr>
                <th scope="row">{{date}}</th>
                <td>{{affiliation}}</td>
                <td>{{affiliation_name}}</td>
                <td>{{participants}}</td>
                <td>{{poster}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
    {{/if}}